{
  "name": "react-skeleton",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "^1.17.1",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.8.6",
    "sinon": "^7.3.2"
  },
  "collectCoverageFrom": [
    "src/**/*.{js,jsx}",
    "!src/serviceWorker.js",
    "!src/index.js"
  ],
  "coverageReporters": [
    "lcov",
    "json",
    "text",
    "text-summary"
  ]
}
